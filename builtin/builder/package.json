{
    "name": "builder",
    "title": "i18n:builder.title",
    "version": "1.2.6",
    "package_version": 2,
    "author": "Cocos Creator",
    "description": "i18n:builder.description",
    "main": "./dist/browser/index",
    "panels": {
        "default": {
            "title": "i18n:builder.title",
            "main": "./dist/panels/index.js",
            "icon": "./static/images/icon.png",
            "size": {
                "min-width": 550,
                "min-height": 400,
                "width": 600,
                "height": 800
            }
        },
        "splash-setting": {
            "title": "splash-setting",
            "main": "./dist/panels/splash-setting/index.js",
            "type": "simple",
            "flags": {
                "resizable": true
            },
            "size": {
                "min-width": 400,
                "min-height": 500,
                "width": 500,
                "height": 600
            }
        },
        "debug-tools": {
            "title": "debug-tools",
            "main": "./dist/panels/debug-tools/index.js",
            "flags": {
                "resizable": true
            },
            "size": {
                "min-width": 400,
                "min-height": 500,
                "width": 500,
                "height": 600
            }
        },
        "certificate": {
            "title": "certificate",
            "main": "./dist/panels/certificate/index.js",
            "type": "simple",
            "flags": {
                "resizable": true
            },
            "size": {
                "min-width": 250,
                "min-height": 200,
                "width": 350,
                "height": 400
            }
        }
    },
    "migrations": [
        {
            "version": "1.2.5",
            "project": "./dist/browser/migration/version/1.2.5.js"
        },
        {
            "version": "1.2.6",
            "project": "./dist/browser/migration/version/1.2.6.js"
        }
    ],
    "contributions": {
        "inspector": "./dist/contributions/inspector/index.js",
        "menu": [
            {
                "path": "i18n:menu.project",
                "label": "i18n:builder.title",
                "message": "open",
                "icon": "./static/images/icon.png",
                "group": "build"
            },
            {
                "path": "i18n:menu.project",
                "label": "i18n:builder.create_user_template",
                "icon": "./static/images/template_download.png",
                "group": "build"
            },
            {
                "path": "i18n:menu.develop",
                "label": "i18n:builder.debug_mode",
                "message": "open-devtools",
                "group": "operation"
            },
            {
                "path": "i18n:menu.project",
                "label": "i18n:builder.create_Build_plugin_template",
                "message": "create-build-plugin-template",
                "icon": "./static/images/template_download.png",
                "group": "build"
            }
        ],
        "shortcuts": [
            {
                "message": "open",
                "mac": "cmd+shift+b",
                "win": "ctrl+shift+b"
            }
        ],
        "profile": {
            "editor": {
                "log.level": {
                    "label": "i18n:builder.preferences.log_level",
                    "message": "preferences-changed",
                    "default": 3
                },
                "supportTaskQueue": {
                    "default": false,
                    "message": "preferences-changed"
                },
                "useBuildAssetCache": {
                    "default": true,
                    "label": "i18n:builder.preferences.useBuildAssetCache"
                },
                "common.name": {
                    "default": ""
                },
                "common.platform": {
                    "default": "web-desktop"
                },
                "common.buildPath": {
                    "default": "./build"
                },
                "common.debug": {
                    "default": false
                },
                "common.md5Cache": {
                    "default": false
                },
                "common.sourceMaps": {
                    "default": false
                },
                "common.replaceSplashScreen": {
                    "default": false
                },
                "common.mainBundleCompressionType": {
                    "default": "merge_dep"
                },
                "common.mainBundleIsRemote": {
                    "default": false
                },
                "common.mergeStartScene": {
                    "default": false
                },
                "common.experimentalEraseModules": {
                    "default": false
                },
                "common.compressTexture": {
                    "default": true
                },
                "common.packAutoAtlas": {
                    "default": true
                },
                "common.startScene": {
                    "default": ""
                },
                "common.scenes": {
                    "default": []
                },
                "common.outputName": {
                    "default": "outputName"
                }
            },
            "project": {
                "textureCompressConfig.userPreset": {
                    "default": {}
                },
                "textureCompressConfig.defaultConfig": {
                    "default": {
                        "default": {
                            "name": "default",
                            "options": {
                                "miniGame": {
                                    "etc1_rgb": "fast",
                                    "pvrtc_4bits_rgb": "fast"
                                },
                                "android": {
                                    "astc_8x8": "-medium",
                                    "etc1_rgb": "fast"
                                },
                                "ios": {
                                    "astc_8x8": "-medium",
                                    "pvrtc_4bits_rgb": "fast"
                                },
                                "web": {
                                    "astc_8x8": "-medium",
                                    "etc1_rgb": "fast",
                                    "pvrtc_4bits_rgb": "fast"
                                },
                                "pc": {}
                            }
                        },
                        "transparent": {
                            "name": "transparent",
                            "options": {
                                "miniGame": {
                                    "etc1_rgb_a": "fast",
                                    "pvrtc_4bits_rgb_a": "fast"
                                },
                                "android": {
                                    "astc_8x8": "-medium",
                                    "etc1_rgb_a": "fast"
                                },
                                "ios": {
                                    "astc_8x8": "-medium",
                                    "pvrtc_4bits_rgb_a": "fast"
                                },
                                "web": {
                                    "astc_8x8": "-medium",
                                    "etc1_rgb_a": "fast",
                                    "pvrtc_4bits_rgb_a": "fast"
                                },
                                "pc": {}
                            }
                        }
                    }
                },
                "splash-setting": {
                    "default": "base64"
                },
                "hasSetSplash": {
                    "default": false
                }
            }
        },
        "preferences": {
            "properties": {
                "log.level": {
                    "ui": "ui-select",
                    "items": [
                        {
                            "label": "i18n:builder.preferences.log_level_error",
                            "value": 1
                        },
                        {
                            "label": "i18n:builder.preferences.log_level_warn",
                            "value": 2
                        },
                        {
                            "label": "i18n:builder.preferences.log_level_log",
                            "value": 3
                        },
                        {
                            "label": "i18n:builder.preferences.log_level_debug",
                            "value": 4
                        }
                    ]
                },
                "useBuildAssetCache": {
                    "ui": "ui-checkbox"
                }
            }
        },
        "project": {
            "compress": {
                "label": "i18n:builder.project.texture_compress.title",
                "custom": "./dist/contributions/compress-texture/project-texture-compress.js"
            }
        },
        "messages": {
            "open": {
                "methods": [
                    "open"
                ]
            },
            "run-button-task": {
                "methods": [
                    "run-button-task"
                ]
            },
            "open-platform-debug-tools": {
