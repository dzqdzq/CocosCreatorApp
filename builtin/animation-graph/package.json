{
    "name": "animation-graph",
    "title": "i18n:animation-graph.title",
    "version": "1.0.0",
    "package_version": 2,
    "author": "Cocos Creator",
    "description": "i18n:animation-graph.description",
    "main": "./dist/browser.js",
    "panels": {
        "default": {
            "title": "i18n:animation-graph.title",
            "main": "./dist/panel.js",
            "icon": "./static/icon-2x.png",
            "flags": {
                "save": true
            },
            "size": {
                "min-width": 450,
                "min-height": 225,
                "width": 900,
                "height": 650
            }
        }
    },
    "contributions": {
        "scene": {
            "script": "./dist/scene.js"
        },
        "inspector": {
            "type": {
                "animation-graph": "./dist/contributions/inspector/index.js"
            }
        },
        "menu": [
            {
                "path": "i18n:menu.panel",
                "label": "i18n:animation-graph.description",
                "icon": "./static/icon.png",
                "message": "open"
            }
        ],
        "profile": {
            "editor": {
                "transition.interruption": {
                    "label": "i18n:animation-graph.transition.interruption",
                    "default": false
                }
            }
        },
        "preferences": {
            "laboratory": [
                "transition.interruption"
            ]
        },
        "shortcuts": [
            {
                "message": "unselect",
                "win": "esc",
                "mac": "esc",
                "when": "PanelName === 'animation-graph'"
            },
            {
                "message": "apply",
                "mac": "cmd+s",
                "win": "ctrl+s",
                "when": "PanelName === 'animation-graph'"
            },
            {
                "message": "delete",
                "win": "delete",
                "mac": "cmd+backspace",
                "when": "PanelName === 'animation-graph'"
            }
        ],
        "messages": {
            "open": {
                "methods": [
                    "open"
                ]
            },
            "dialog-warn": {
                "methods": [
                    "dialogWarn"
                ]
            },
            "apply": {
                "methods": [
                    "default.apply"
                ]
            },
            "unselect": {
                "methods": [
                    "default.unselect"
                ]
            },
            "delete": {
                "methods": [
                    "default.delete"
                ]
            },
            "scene:ready": {
                "methods": [
                    "default.refresh"
                ]
            },
            "animation-graph:changed": {
                "methods": [
                    "default.refresh"
                ]
            }
        }
    }
}