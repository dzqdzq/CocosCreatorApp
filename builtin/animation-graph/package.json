{
    "name": "animation-graph",
    "title": "i18n:animation-graph.title",
    "version": "1.0.0",
    "package_version": 2,
    "author": "Cocos Creator",
    "description": "i18n:animation-graph.description",

    "main": "./dist/browser.js",

    "panels": {
        "default": {
            "title": "i18n:animation-graph.title",
            "main": "./dist/panel.js",
            "icon": "./static/icon.png",

            "flags": {
                "save": true
            },

            "size": {
                "min-width": 450,
                "min-height": 225,
                "width": 900,
                "height": 650
            }
        }
    },

    "contributions": {
        "scene": {
            "script": "./dist/scene.js"
        },
        "menu": [
            {
                "path": "i18n:menu.panel",
                "label": "i18n:animation-graph.description",
                "icon": "./static/icon.png",
                "message": "open"
            }
        ],
        "profile": {
            "editor": {
                "laboratory": {
                    "label": "i18n:animation-graph.laboratory",
                    "default": true
                }
            }
        },
        "preferences": {
            "laboratory": ["laboratory"]
        },
        "shortcuts": [
            {
                "message": "unselect",
                "win": "esc",
                "mac": "esc",
                "when": "PanelName === 'animation-graph'"
            },
            {
                "message": "apply",
                "mac": "cmd+s",
                "win": "ctrl+s",
                "when": "PanelName === 'animation-graph'"
            },
            {
                "message": "delete",
                "win": "delete",
                "mac": "delete",
                "when": "PanelName === 'animation-graph'"
            }
        ],
        "messages": {
            "open": {
                "methods": ["open"]
            },
            "dialog-warn": {
                "methods": ["dialogWarn"]
            },
            "apply": {
                "methods": ["default.apply"]
            },
            "refresh": {
                "methods": ["default.refresh"]
            },
            "scene:ready": {
                "methods": ["default.refresh"]
            },
            "unselect": {
                "methods": ["default.unselect"]
            },
            "delete": {
                "methods": ["default.delete"]
            }
        }
    }
}
