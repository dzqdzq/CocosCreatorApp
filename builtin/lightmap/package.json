{
    "name": "lightmap",
    "title": "i18n:lightmap.title",
    "version": "1.0.0",
    "package_version": 2,
    "author": "Cocos Creator",
    "main": "./dist/browser.js",
    "description": "i18n:lightmap.description",

    "panels": {
        "panel": {
            "title": "i18n:lightmap.title",
            "icon": "./static/icon.png",
            "main": "./dist/panel/index.js",
            "size": {
                "min-width": 400,
                "min-height": 400,
                "width": 400,
                "height": 600
            }
        }
    },

    "contributions": {
        "menu": [
            {
                "path": "i18n:menu.project",
                "label": "i18n:lightmap.title",
                "message": "open",
                "icon": "./static/icon.png"
            }
        ],

        "profile": {
            "editor": {
                "laboratory.bake_feature": {
                    "label": "i18n:preferences.laboratory.bake_feature",
                    "default": true
                }
            }
        },

        "preferences": {
            "laboratory": ["laboratory.bake_feature"]
        },

        "messages": {
            "open": {
                "methods": ["open"]
            },
            "unstaging": {
                "methods": ["unstaging"]
            },
            "savePicPath": {
                "methods": ["savePicPath"]
            },
            "clear": {
                "methods": ["clear"]
            },

            "scene:lightmap-start": {
                "methods": ["start", "panel.start"]
            },
            "scene:lightmap-cancel": {
                "methods": ["cancel", "panel.cancel"]
            },
            "scene:lightmap-log": {
                "methods": ["log", "panel.log"]
            },
            "scene:lightmap-finished": {
                "methods": ["finished", "panel.finished"]
            },
            "scene:lightmap-end": {
                "methods": ["end", "panel.end"]
            },
            "scene:lightmap-progress": {
                "methods": ["progress", "panel.progress"]
            }
        }
    }
}
