{
    "package_version": 2,
    "version": "1.0.0",
    "name": "localization-editor",
    "description": "i18n:localization-editor.description",
    "main": "./dist/main/main.js",
    "author": "Cocos Creator",
    "editor": ">=3.6.0",
    "scripts": {
        "postinstall": "node clear.js",
        "build": "npx tsc -b --verbose && npx vite build --mode development",
        "build:dev": "npx tsc -b --verbose && npx vite build --mode development",
        "build:prod": "npx tsc -b --verbose && npx vite build --mode production && cd static && npx tsc -b",
        "watch": "npx npm run vite:watch & npx npm run tsc:watch",
        "vite:build": "npx vite build --mode development",
        "vite:watch": "npx vite build --watch --mode development",
        "tsc:watch": "npx tsc -w",
        "test": "npx jest",
        "generate-icu-option": "npx ts-node ./bin/generate-icu-option.ts",
        "generate-main-ipc": "npx ts-node ./bin/generate-main-ipc.ts"
    },
    "dependencies": {
        "@formatjs/intl": "^2.3.0",
        "@formatjs/intl-datetimeformat": "^5.0.1",
        "@formatjs/intl-displaynames": "^5.4.3",
        "@formatjs/intl-getcanonicallocales": "^1.9.2",
        "@formatjs/intl-listformat": "^7.0.1",
        "@formatjs/intl-locale": "^2.4.47",
        "@formatjs/intl-numberformat": "^7.4.3",
        "@formatjs/intl-relativetimeformat": "^10.0.1",
        "i18next": "^21.6.16",
        "intl-pluralrules": "^1.3.1"
    },
    "devDependencies": {
        "js-yaml": "^4.1.0",
        "@types/js-yaml": "^4.0.5",
        "@cocos-fe/eslint-config": "^1.0.27",
        "@types/crypto-js": "^4.1.1",
        "@types/eslint": "^8.4.5",
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^9.0.5",
        "@types/glob": "7.2.0",
        "@types/jest": "^27.5.2",
        "@types/lodash": "^4.14.182",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "@vitejs/plugin-vue": "^2.3.1",
        "axios": "^0.27.2",
        "crypto-js": "^4.1.1",
        "csv": "^6.2.0",
        "eslint": "^7.15.0",
        "eslint-plugin-jsdoc": "^30.7.8",
        "eslint-plugin-mocha": "^10.0.4",
        "eslint-plugin-vue": "^9.2.0",
        "express": "^4.18.1",
        "fs-extra": "^10.1.0",
        "gettext-extractor": "^3.5.4",
        "gettext-parser": "^5.1.2",
        "glob": "7.2.0",
        "jest": "^28.1.2",
        "less": "^4.1.2",
        "reflect-metadata": "^0.1.13",
        "ts-jest": "^28.0.5",
        "ts-morph": "^15.1.0",
        "ts-node": "^10.7.0",
        "tsyringe": "^4.6.0",
        "vite": "^2.9.5",
        "vue": "^3.2.25",
        "vue-eslint-parser": "^8.0.1",
        "xlsx": "^0.18.5",
        "yaml": "^2.1.0"
    },
    "panels": {
        "default": {
            "title": "Localization Editor",
            "type": "dockable",
            "main": "panel-dist/default/index.js",
            "icon": "./static/icon-2x.png",
            "size": {
                "min-width": 253,
                "min-height": 750,
                "width": 1280,
                "height": 1200
            }
        }
    },
    "contributions": {
        "inspector": {
            "section": {
                "node": {
                    "L10nLabel": "panel-dist/l10n-label-inspector/index.js",
                    "ICUComponent": "dist/inspector/ICUComponentInspector.js"
                }
            }
        },
        "menu": [
            {
                "path": "i18n:menu.panel",
                "label": "i18n:localization-editor.localization-editor",
                "icon": "./static/icon.png",
                "message": "open-panel"
            }
        ],
        "messages": {
            "execute-panel-method": {
                "methods": [
                    "default.executePanelMethod"
                ]
            },
            "scene:ready": {
                "methods": [
                    "onSceneReady"
                ]
            },
            "toggle": {
                "methods": [
                    "toggle"
                ]
            },
            "enable-changed": {
                "methods": [
                    "enableChanged"
                ]
            },
            "get-enable": {
                "methods": [
                    "getEnable"
                ]
            },
            "open-panel": {
                "methods": [
                    "openPanel"
                ]
            },
            "preview-by": {
                "methods": [
                    "previewBy"
                ]
            },
            "scan": {
                "methods": [
                    "scan"
                ]
            },
            "uninstall": {
                "methods": [
                    "uninstall"
                ]
            },
            "read-config": {
                "methods": [
                    "readConfig"
                ]
            },
            "get-index-data": {
                "methods": [
                    "getIndexData"
                ]
            },
            "get-local-language": {
                "methods": [
                    "getLocalLanguage"
                ]
            },
            "get-translate-data": {
                "methods": [
                    "getTranslateData"
                ]
            },
            "get-translate-data-object": {
                "methods": [
                    "getTranslateDataObject"
                ]
            },
            "save-translate-data": {
                "methods": [
                    "saveTranslateData"
                ]
            },
            "clear-translate-data": {
                "methods": [
                    "clearTranslateData"
                ]
            },
            "set-local-language-locale": {
                "methods": [
                    "setLocalLanguageLocale"
                ]
            },
            "set-language-config": {
                "methods": [
                    "setLanguageConfig"
                ]
            },
            "get-language-config": {
                "methods": [
                    "getLanguageConfig"
                ]
            },
            "get-all-language-configs": {
                "methods": [
                    "getAllLanguageConfigs"
                ]
            },
            "add-target-language": {
                "methods": [
                    "addTargetLanguage"
                ]
            },
            "remove-target-language": {
                "methods": [
                    "removeTargetLanguage"
                ]
            },
            "get-translate-providers": {
                "methods": [
                    "getTranslateProviders"
                ]
            },
            "get-translate-provider-supported-languages": {
                "methods": [
                    "getTranslateProviderSupportedLanguages"
                ]
            },
            "get-current-translate-provider": {
                "methods": [
                    "getCurrentTranslateProvider"
                ]
            },
            "get-translate-provider": {
                "methods": [
                    "getTranslateProvider"
                ]
            },
            "set-current-translate-provider": {
                "methods": [
                    "setCurrentTranslateProvider"
                ]
            },
            "clear-translate-provider": {
                "methods": [
                    "clearTranslateProvider"
                ]
            },
            "change-value": {
                "methods": [
                    "changeValue"
                ]
            },
            "get-scan-options": {
                "methods": [
                    "getScanOptions"
                ]
            },
            "auto-translate": {
                "methods": [
                    "autoTranslate"
                ]
            },
            "import-media-files": {
                "methods": [
                    "importMediaFiles"
                ]
            },
            "compile": {
                "methods": [
                    "compile"
                ]
            },
            "add-association": {
                "methods": [
                    "addAssociation"
                ]
            },
            "remove-association": {
                "methods": [
                    "removeAssociation"
                ]
            },
            "get-resource-list": {
                "methods": [
                    "getResourceList"
                ]
            },
            "get-resource-bundle": {
                "methods": [
                    "getResourceBundle"
                ]
            },
            "import-translate-file": {
                "methods": [
                    "importTranslateFile"
                ]
            },
            "export-translate-file": {
                "methods": [
                    "exportTranslateFile"
                ]
            }
        },
        "asset-db": {
            "mount": {
                "path": "./static/assets",
                "readonly": true,
                "visible": true,
                "enable": "L10nEnable"
            }
        },
        "scene": {
            "script": "./dist/scene/scene.js"
        },
        "profile": {
            "project": {
                "ProjectConfig": {
                    "default": {},
                    "label": "i18n:localization_editor_project_config"
                },
                "L10nEnable": {
                    "default": false,
                    "label": "i18n:localization_editor_project_config",
                    "message": "enable-changed"
                }
            },
            "editor": {
                "EditorConfig": {
                    "default": {},
                    "label": "i18n:localization_editor_editor_config"
                },
                "TranslateProviderConfigMap": {
                    "default": {},
                    "label": "i18n:localization_editor_provider_config"
                }
            }
        },
        "builder": "./dist/builder/builder.js",
        "server": "./dist/server/server.js"
    }
}
